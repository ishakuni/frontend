{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "author": {
    "email": "caulagi@gmail.com",
    "name": "Pradip Caulagi",
    "url": "https://caulagi.com"
  },
  "license": "MIT",
  "dependencies": {
    "bs-css": "^8.0.1",
    "gatsby": "^2.0.117",
    "react": "^16.8.1",
    "react-dom": "^16.8.3",
    "reason-react": "^0.5.3"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:bsb": "bsb -make-world",
    "build:gatsy": "gatsby build",
    "dev": "run-p dev:*",
    "dev:bsb": "yarn build:bsb -w",
    "dev:gatsby": "gatsby develop",
    "pretest": "bsb -make-world",
    "test": "yarn jest",
    "clean": "bsb -clean-world && rimraf build .cache .merlin lib public *.js static/js",
    "format": "bsrefmt --in-place src/**/**.re src/**/__tests__/**.re"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@glennsl/bs-jest": "^0.4.7",
    "@wyze/gatsby-plugin": "^1.0.0",
    "bs-fetch": "^0.3.1",
    "bs-jest-dom": "^1.0.0",
    "bs-platform": "4.0.5",
    "bs-react-testing-library": "^0.4.0",
    "gatsby-source-filesystem": "^2.0.22",
    "jest-dom": "^3.1.2",
    "npm-run-all": "^4.1.5",
    "react-testing-library": "^5.9.0",
    "rimraf": "^2.6.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ishakuni/frontend"
  },
  "bugs": {
    "url": "https://github.com/ishakuni/frontend/issues"
  }
}
