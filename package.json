{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "author": {
    "email": "caulagi@gmail.com",
    "name": "Pradip Caulagi",
    "url": "https://caulagi.com"
  },
  "license": "MIT",
  "scripts": {
    "build": "run-s build:*",
    "build:bsb": "bsb -make-world",
    "build:config": "run-s build:config:*",
    "build:config:browser": "echo 'import \"./src/styles/global.bs.js\"' > gatsby-browser.js",
    "build:config:config": "rio -o gatsby-config.js src/config/gatsby.re",
    "build:gatsby": "gatsby build",
    "clean": "bsb -clean-world && rimraf .cache .merlin lib public *.js static/js",
    "dev": "run-p dev:*",
    "dev:bsb": "yarn build:bsb -w",
    "dev:gatsby": "gatsby develop",
    "format": "bsrefmt --in-place src/**/**.re src/**/__tests__/**.re",
    "jest": "jest",
    "lh": "lighthouse-ci",
    "predev:gatsby": "yarn build:config",
    "prejest": "rio -o babel.config.js src/config/babel.re",
    "pretest": "yarn build:bsb",
    "serve": "gatsby serve",
    "test": "yarn jest"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@glennsl/bs-jest": "^0.4.8",
    "@moox/bs-react-helmet": "^1.1.3",
    "@wyze/gatsby-plugin": "^1.0.0",
    "@wyze/rio": "^1.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "bs-css": "^8.0.4",
    "bs-jest-dom": "^2.0.1",
    "bs-platform": "^5.0.4",
    "bs-react-test-renderer": "^2.0.0",
    "bs-react-testing-library": "^0.5.0",
    "gatsby": "2.0.22",
    "gatsby-plugin-manifest": "^2.0.29",
    "gatsby-plugin-offline": "^2.1.0",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-filesystem": "^2.0.33",
    "husky": "^2.3.0",
    "jest-dom": "^3.2.2",
    "lighthouse-ci": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "reason-react": "^0.5.3",
    "rimraf": "^2.6.3"
  },
  "browserslist": [
    "cover 99.5%"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ishakuni/frontend"
  },
  "bugs": {
    "url": "https://github.com/ishakuni/frontend/issues"
  },
  "jest": {
    "roots": [
      "src"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(@ahrefs/bs-dotenv|@glennsl/bs-jest|@moox/bs-react-helmet|bs-|reason-react))"
    ]
  }
}
