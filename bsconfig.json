{
  "name": "ishakuni-frontend",
  "sources": [
    {
      "dir": "src",
      "subdirs": [
        {
          "dir": "components",
          "subdirs": [
            {
              "dir": "__tests__",
              "type": "dev"
            }
          ]
        },
        {
          "dir": "pages",
          "subdirs": [
            {
              "dir": "__tests__",
              "type": "dev"
            }
          ]
        },
        "styles",
        "config"
      ]
    }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": false
  },
  "suffix": ".js",
  "reason": {
    "react-jsx": 2
  },
  "bs-dependencies": [
    "bs-css",
    "reason-react",
    "@moox/bs-react-helmet"
  ],
  "bs-dev-dependencies": [
    "@glennsl/bs-jest",
    "bs-jest-dom",
    "bs-react-testing-library",
    "bs-react-test-renderer"
  ],
  "bsc-flags": [
    "-bs-no-version-header",
    "-bs-super-errors"
  ],
  "refmt": 3
}
